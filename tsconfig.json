{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "types": ["./src/env.d.ts"],
      "exactOptionalPropertyTypes": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  